@startuml
'https://plantuml.com/class-diagram

class Pos {
     + id: Long
     + createdAt: LocalDateTime
     + updatedAt: LocalDateTime
     + name: String
     + description: String
     + type: PosType
     + campus: CampusType
     + street: String
     + houseNumber: String
     + postalCode: Integer
     + city: String
}
class User{
    + id: Long
    + createdAt: LocalDateTime
    + updatedAt: LocalDateTime
    + loginName: String
    + emailAddress: String
    + firstName: String
    + lastName: String
}
class Review{
    + id: Long
    + createdAt: LocalDateTime
    + updatedAt: LocalDateTime
    + review: String
    + approvalCount: Integer
    + approved: Boolean

    }
class OSMNode{
    + nodeId: Long
   }

    Review "1" --> "1" User: author
    Review "1" --> "1" Pos: assignedTo
    Pos "1" --> "0...*" Review: hasReviews
    User "1" --> "0...*" Review: madeReviews
    Pos "0..1" --> "1" OSMNode: imported


@enduml